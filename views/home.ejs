<%- include("partials/header") %>

    <h1>Home</h1>
    <%= homeStartingContent %>

    <% posts.forEach(post => { %>
        <h1><%= post.title %> </h1>
        <p> 
            <%= post.content.substring(0, 100) %>... 
            <a href="/posts/<%=post._id%>">Read More</a>
        </p>
    <% }) %> 

